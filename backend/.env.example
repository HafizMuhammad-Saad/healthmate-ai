# HealthMate AI - Environment Variables

# Server Configuration
PORT=5000
ORIGIN=http://localhost:5173

# Database
MONGO_URI=mongodb://localhost:27017/healthmate_ai
# or MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/healthmate_ai

# Authentication
JWT_SECRET=your_jwt_secret_key_here_make_it_strong

# Cloudinary Configuration (for file uploads)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
# Alternative: OPENAI_API_KEY=your_openai_api_key_here

# Development
NODE_ENV=development